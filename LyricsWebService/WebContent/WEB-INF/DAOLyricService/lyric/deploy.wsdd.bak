<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from DAOLyricService WSDL service -->

  <service name="DAOLyric" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://lyric"/>
      <parameter name="wsdlServiceElement" value="DAOLyricService"/>
      <parameter name="schemaQualified" value="http://lyric"/>
      <parameter name="wsdlServicePort" value="DAOLyric"/>
      <parameter name="className" value="lyric.DAOLyricSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="DAOLyric"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getAPILyric" qname="operNS:getAPILyric" xmlns:operNS="http://lyric" returnQName="retNS:getAPILyricReturn" xmlns:retNS="http://lyric" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:songName" xmlns:pns="http://lyric" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:artistName" xmlns:pns="http://lyric" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getStaticLyric" qname="operNS:getStaticLyric" xmlns:operNS="http://lyric" returnQName="retNS:getStaticLyricReturn" xmlns:retNS="http://lyric" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:songName" xmlns:pns="http://lyric" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:artistName" xmlns:pns="http://lyric" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getAPILyric getStaticLyric"/>

  </service>
</deployment>
